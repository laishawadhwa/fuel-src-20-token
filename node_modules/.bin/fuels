#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/laishawadhwa/Documents/allthingsfuel/dev/NFT-matt/sway-applications/node_modules/.pnpm/fuels@0.90.0/node_modules/fuels/node_modules:/Users/laishawadhwa/Documents/allthingsfuel/dev/NFT-matt/sway-applications/node_modules/.pnpm/fuels@0.90.0/node_modules:/Users/laishawadhwa/Documents/allthingsfuel/dev/NFT-matt/sway-applications/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/laishawadhwa/Documents/allthingsfuel/dev/NFT-matt/sway-applications/node_modules/.pnpm/fuels@0.90.0/node_modules/fuels/node_modules:/Users/laishawadhwa/Documents/allthingsfuel/dev/NFT-matt/sway-applications/node_modules/.pnpm/fuels@0.90.0/node_modules:/Users/laishawadhwa/Documents/allthingsfuel/dev/NFT-matt/sway-applications/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../fuels/fuels.js" "$@"
else
  exec node  "$basedir/../fuels/fuels.js" "$@"
fi
